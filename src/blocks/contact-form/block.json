{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "sg/contact",
  "title": "SG Formulaire de contact",
  "category": "sg-blocks",
  "icon": "email",
  "attributes": {
    "form_id": {
      "type": "string",
      "source": "attribute",
      "selector": "form",
      "attribute": "id"
    },
    "title": {
      "type": "string",
      "source": "text",
      "selector": "h1,h2,h3,h4,h5,h6"
    },
    "subtitle": {
      "type": "string",
      "source": "text",
      "selector": "p"
    },
    "ctaText": {
      "type": "string",
      "default": "Envoyer",
      "source": "text",
      "selector": "button[type=submit]"
    },
    "titleLevel": {
      "type": "number",
      "default": 2
    },
    "iconCode": {
      "type": "string"
    },
    "successMsg": {
      "type": "string"
    },
    "errorMsg": {
      "type": "string"
    }   
  },
  "textdomain": "sg-blocks",
  "editorStyle": "file:./editor.css",
  "style": "file:./style.css"
}
